        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate Unique User Credentials">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="script">// Check if variables already exist for this thread
if (vars.get(&quot;user_email&quot;) == null) {
    String threadNum = ctx.getThreadNum().toString();
    String iterationNum = vars.getIteration() != null ? vars.getIteration().toString() : &quot;0&quot;;
    String uniqueId = UUID.randomUUID().toString().substring(0, 8);
    String uniqueKey = threadNum + &quot;_&quot; + iterationNum;
    
    String email = &quot;user_&quot; + uniqueKey + &quot;_&quot; + uniqueId + iterationNum + &quot;@example.com&quot;;
    String password = &quot;pass_&quot; + uniqueKey;
    String name = &quot;User &quot; + threadNum;
    
    vars.put(&quot;user_email&quot;, email);
    vars.put(&quot;user_password&quot;, password);
    vars.put(&quot;user_name&quot;, name);
    
    log.info(&quot;Generated NEW user variables for thread &quot; + threadNum + &quot;, iteration &quot; + iterationNum);
    log.info(&quot;User email: &quot; + email);
    log.info(&quot;User password: &quot; + password);
} else {
    log.info(&quot;REUSING existing user variables for thread &quot; + ctx.getThreadNum());
    log.info(&quot;User email: &quot; + vars.get(&quot;user_email&quot;));
    log.info(&quot;User password: &quot; + vars.get(&quot;user_password&quot;));
}</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
        </JSR223PreProcessor>
