        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate Unique Restaurant Credentials">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">// Check if variables already exist for this thread
if (vars.get(&quot;restaurant_email&quot;) == null) {
    try {
        String timestamp = System.currentTimeMillis().toString();
        String threadNum = ctx.getThreadNum().toString();
        String uniqueId = UUID.randomUUID().toString().substring(0, 8);
        String iterationNum = vars.getIteration() != null ? vars.getIteration().toString() : &quot;0&quot;;
        
        String uniqueKey = threadNum + &quot;_&quot; + iterationNum;
        
        String restaurantEmail = &quot;restaurant_&quot; + uniqueKey + &quot;_&quot; + uniqueId + &quot;_&quot; + iterationNum + &quot;@example.com&quot;;
        String restaurantPassword = &quot;pass_&quot; + uniqueKey;
        String restaurantName = &quot;Restaurant &quot; + threadNum + &quot; &quot; + uniqueId;
        String phoneNumber = &quot;555-&quot; + timestamp.substring(timestamp.length() - 7);
        
        vars.put(&quot;restaurant_email&quot;, restaurantEmail);
        vars.put(&quot;restaurant_password&quot;, restaurantPassword);
        vars.put(&quot;restaurant_name&quot;, restaurantName);
        vars.put(&quot;restaurant_phone&quot;, phoneNumber);
        
        vars.put(&quot;restaurant_cuisine&quot;, &quot;Italian&quot;);
        vars.put(&quot;restaurant_category&quot;, &quot;Fine Dining&quot;);
        vars.put(&quot;restaurant_location&quot;, &quot;New York&quot;);
        vars.put(&quot;restaurant_address&quot;, &quot;123 Test St &quot; + threadNum);
        vars.put(&quot;restaurant_description&quot;, &quot;Test restaurant &quot; + uniqueId);
        vars.put(&quot;restaurant_business_hours&quot;, &quot;9AM-10PM&quot;);
        vars.put(&quot;restaurant_price_range&quot;, &quot;\$\$&quot;);
        vars.put(&quot;restaurant_delivery_time&quot;, &quot;30 min&quot;);
        
        log.info(&quot;Generated NEW restaurant variables for thread &quot; + threadNum + &quot;, iteration &quot; + iterationNum);
        log.info(&quot;Restaurant email: &quot; + restaurantEmail);
        log.info(&quot;Restaurant password: &quot; + restaurantPassword);
    } catch (Exception e) {
        log.error(&quot;Error in PreProcessor: &quot; + e.getMessage(), e);
        throw e;
    }
} else {
    log.info(&quot;REUSING existing restaurant variables for thread &quot; + ctx.getThreadNum());
    log.info(&quot;Restaurant email: &quot; + vars.get(&quot;restaurant_email&quot;));
    log.info(&quot;Restaurant password: &quot; + vars.get(&quot;restaurant_password&quot;));
}</stringProp>
